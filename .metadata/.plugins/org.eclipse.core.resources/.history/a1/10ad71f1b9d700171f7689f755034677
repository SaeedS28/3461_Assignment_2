import java.util.ArrayList;


public class Course extends GeneralCourse
{
	private String courseTerm;
	private String courseDesc;
	private String courseInstructor;
	private String courseLocation; 
	private ArrayList<CourseSchedule> schedule;

	
	
	
	public Course(String courseName, int courseCredits, String courseTerm, String courseDesc, ArrayList<String> daysOffered, String courseInstructor, String courseLocation)
	{
		super(courseName, courseCredits);
		this.setCourseTerm(courseTerm);
		this.setCourseDesc(courseDesc);
		this.setDaysOffered(daysOffered);
		this.setCourseInstructor(courseInstructor);
	}
	
	public String getCourseTerm()
	{
		return this.courseTerm;
	}
	
	public void setCourseTerm(String courseTerm)
	{
		this.courseTerm = courseTerm;
	}
	
	public String getCourseDesc()
	{
		return this.courseDesc;
	}
	
	public void setCourseDesc(String courseDesc)
	{
		this.courseDesc = courseDesc;
	}
	
	
	public String getCourseInstructor()
	{
		return this.courseInstructor;
	}
	
	public void setCourseInstructor(String courseInstructor)
	{
		this.courseInstructor = courseInstructor;
	}
}