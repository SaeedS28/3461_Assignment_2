import java.util.*;


public class Degree 
{
	private String degreeName;
	private String degreeFaculty;
	private String degreeType;
	private int degreeDuration;
	private int totalCredits;
	private ArrayList<Course> degreeCourses;
	
	public Degree(String degreeName, String degreeFaculty, String degreeType, int degreeDuration, int totalCredits, ArrayList<Course> degreeCourses)
	{
		
	}
	
	public String getDegreeName()
	{
		return this.degreeName;
	}
	
	public void setDegreeName(String degreeName)
	{
		this.degreeName = degreeName;
	}
	
	public String getDegreeFaculty()
	{
		return this.degreeType;
	}
	
	public void setDegreeFaculty(String degreeFaculty)
	{
		this.degreeFaculty = degreeFaculty;
	}
	
	public String getDegreeType()
	{
		return this.degreeType;
	}
	
}
