import java.util.*;

public class CourseSuggestionOption2 
{
	
	private Course course;
	
	public CourseSuggestionOption2(Course course)
	{
		this.setCourse(course);
	}
	
	public Course getCourse()
	{
		return this.course;
	}
	
	public void setCourse(Course course)
	{
		this.course = course;
	}

	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return this.getCourse().getGeneralCourseName() + " (" + this.getCourse().getCourseTerm() + ")";
	}
	


}
